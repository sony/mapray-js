{
  "name": "@mapray/ui",
  "version": "0.8.8",
  "description": "UI Engine for mapray-js",
  "keywords": [
    "3D",
    "sdk",
    "api",
    "webgl",
    "map",
    "geospatial",
    "globe",
    "ui",
    "operation"
  ],
  "main": "dist/umd/maprayui.js",
  "unpkg": "dist/umd/maprayui.min.js",
  "module": "dist/es/maprayui.js",
  "types": "dist/es/@type/index.d.ts",
  "directories": {
    "doc": "doc"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rollup -c",
    "build-watch": "rollup -c -w",
    "css": "postcss -c ./postcss.config.js -o dist/mapray.css src/css/mapray.css",
    "css-dev": "postcss --watch -c ./postcss.config.js -o dist/mapray.css src/css/mapray.css",
    "typedoc": "typedoc --tsconfig tsconfig.json"
  },
  "contributors": [
    {
      "name": "Yasuhisa Tanioka"
    },
    {
      "name": "Daisuke Matsumoto",
      "url": "https://github.com/daisuke201"
    },
    {
      "name": "masa-sony"
    }
  ],
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/sony/mapray-js.git"
  },
  "bugs": {
    "url": "https://github.com/sony/mapray-js/issues"
  },
  "homepage": "https://mapray.com",
  "devDependencies": {
    "@babel/core": "^7.8.4",
    "@babel/preset-env": "^7.8.4",
    "@rollup/plugin-typescript": "^8.2.1",
    "babel-loader": "^8.0.6",
    "core-js": "^3.6.4",
    "cross-env": "^7.0.0",
    "cssnano": "^4.1.10",
    "postcss-cli": "^7.1.0",
    "postcss-inline-svg": "^4.1.0",
    "rollup": "^1.31.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-terser": "^5.2.0",
    "webpack": "^4.41.6",
    "webpack-cli": "^3.3.11",
    "tslib": "^2.2.0",
    "typedoc": "0.21.0-beta.3",
    "typescript": "4.3.2"
  },
  "dependencies": {},
  "peerDependencies": {
    "@mapray/mapray-js": "^0.8.8"
  }
}